# config/database_<%= file_name %>.yml
 
development:
  adapter: sqlite3
  database: db_<%= file_name %>/<%= file_name %>_development.sqlite
  timeout: 5000

test:
  adapter: sqlite3
  database: db_<%= file_name %>/<%= file_name %>_test.sqlite
  timeout: 5000

production:
  adapter: sqlite3
  database: db_<%= file_name %>/<%= file_name %>_production.sqlite
  timeout: 5000
